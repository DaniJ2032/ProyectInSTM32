    "version": "0.2.0",
    "projectName": "3_TIMER1",
    "configurations": [
        {
            "name": "STlink launch",
            "cwd": "${workspaceRoot}",
            "executable": "${workspaceRoot}/build/3_TIMER1.elf",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "stutil",
            "device": "STM32F103C876",
            "interface": "swd",
            "runToMain": true, // else it starts at reset handler - not interested
            "preLaunchTask": "build all", // configured in tasks.json
            // "preLaunchCommands": ["Build all"], // you can execute command instead of task
            "svdFile": "X:\\ELECTRONICA\\ST\\STM32CubeIDE_1.13.1\\STM32CubeIDE\\plugins\\com.st.stm32cube.ide.mcu.productdb.debug_2.1.0.202306151215\\resources\\cmsis\\STMicroelectronics_CMSIS_SVD", // Include svd to watch device peripherals
            "swoConfig": {} // currently (v1.7.0) not supported
        },
        {
            "name": "STlink attach",
            "cwd": "${workspaceRoot}",
